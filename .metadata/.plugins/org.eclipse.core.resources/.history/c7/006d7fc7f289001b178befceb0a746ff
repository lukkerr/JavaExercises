package sorteio;
import java.util.Scanner;

public class Teste {
	public static void main(String[] args) {
		int n = new Scanner("Digite a quantidade maxíma: ");
		int min = new Scanner("Digite o valor mínimo: ");
		int max = new Scanner("Digite o valor maxíma: ");
		
		new Sorteio(n, min, max);
	}
}
